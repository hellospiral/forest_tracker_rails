<%= render('layouts/errors', :object => @sighting) %>

<%= form_for [@animal, @sighting] do |f| %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.label :lat %>
  <%= f.text_field :lat %>
  <%= f.label :long %>
  <%= f.text_field :long %>
  <%= f.collection_select :region_id, Region.all, :id, :name, prompt: true %>
  <p><%= f.submit %></p>
<% end %>
